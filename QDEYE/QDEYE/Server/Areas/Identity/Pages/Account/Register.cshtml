@page
@model RegisterModel
@{
    Layout = null;
}
<!DOCTYPE html>
<!--  Last Published: Mon Jun 01 2020 04:17:47 GMT+0000 (Coordinated Universal Time)  -->
<html data-wf-page="5ec66de82a87854cb6206fe3" data-wf-site="5ec2d8ba5aab1b0a2602941c">
<head>
    <meta charset="utf-8">
    <title>Digital Eye - Регистрация</title>
    <meta content="Digital Eye - Регистрация" property="og:title">
    <meta content="Digital Eye - Регистрация" property="twitter:title">
    <meta content="width=device-width, initial-scale=1" name="viewport">
    <link href="~/css/normalize.css" rel="stylesheet" type="text/css">
    <link href="~/css/components.css" rel="stylesheet" type="text/css">
    <link href="~/css/digitaleyekz.css" rel="stylesheet" type="text/css">
    <!-- [if lt IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.3/html5shiv.min.js" type="text/javascript"></script><![endif] -->
    <script type="text/javascript">!function (o, c) { var n = c.documentElement, t = " w-mod-"; n.className += t + "js", ("ontouchstart" in o || o.DocumentTouch && c instanceof DocumentTouch) && (n.className += t + "touch") }(window, document);</script>
    <link href="~/images/favicon.png" rel="shortcut icon" type="image/x-icon">
    <link href="~/images/webclip.jpg" rel="apple-touch-icon">
</head>
<body class="body-employee">
    <div class="header-auth">
        <div class="de-header-left"><a href="#" class="header-logo w-inline-block"><img src="~/images/de-logo.svg" alt="Digital Work - Удобный инструмент для онлайн трудоустройства" class="image-2"></a></div>
    </div>
    <div class="main-content">
        <div class="form w-container">
            <h1 class="text-header">Регистрация</h1>
            <div class="form-bg">
                <div class="form-register w-form">
                    <form asp-route-returnUrl="@Model.ReturnUrl" method="post" id="wf-form-SignIn" name="wf-form-SignIn" data-name="SignIn" class="form-2">
                        @*<form id="wf-form-SignIn" name="wf-form-SignIn" data-name="SignIn" redirect="/digital-eye-user-data" data-redirect="/digital-eye-user-data" class="form-2">*@
                        <label for="node" class="field-label">Электронная почта</label>
                        <input type="email" asp-for="Input.Email" class="field w-input" autofocus="true" maxlength="256" data-name="Электронная почта" placeholder="example@gmail.com" id="email" required="" />
                        <span asp-validation-for="Input.Email" class="text-danger"></span>
                        @*<input type="email" class="field w-input" autofocus="true" maxlength="256" data-name="Электронная почта" placeholder="example@gmail.com" id="email" required="">*@
                        <label for="node" class="field-label">Введите пароль</label>
                        <input asp-for="Input.Password" type="password" class="field w-input" maxlength="256" data-name="Пароль" placeholder="" id="pwd" required="" />
                        <span asp-validation-for="Input.Password" class="text-danger"></span>
                        @*<input type="password" class="field w-input" maxlength="256" data-name="Пароль" placeholder="" id="pwd" required="">*@
                        <label for="node-2" class="field-label">Повторите пароль</label>
                        <input asp-for="Input.ConfirmPassword" class="field w-input" maxlength="256" data-name="Пароль" placeholder="" id="pwd" required="" />
                        <span asp-validation-for="Input.ConfirmPassword" class="text-danger"></span>
                        @*<input type="password" class="field w-input" maxlength="256" data-name="Пароль" placeholder="" id="pwd" required="">*@
                        <div class="form-row">
                            <label class="w-checkbox checkbox">
                                <div class="w-checkbox-input w-checkbox-input--inputType-custom chekbox-tick w--redirected-checked"></div>
                                <input type="checkbox" id="Запомнить меня" checked="" data-name="Запомнить меня" style="opacity:0;position:absolute;z-index:-1">
                                <span for="Запомнить меня" class="checkbox-label w-form-label">Я согласен с правилами сервиса</span>
                            </label>
                            <a href="#" class="link-block w-inline-block"></a>
                        </div>
                        <div class="w-form-done">
                            <div>Thank you! Your submission has been received!</div>
                        </div>
                        <div class="w-form-fail">
                            <div>Oops! Something went wrong while submitting the form.</div>
                        </div>
                        <button type="submit" class="de-btn-80 fullwidth w-button">Зарегистрироваться</button>
                        @*<a href="digital-eye-user-data.html" class="de-btn-80 fullwidth w-button">Зарегистрироваться</a>*@
                    </form>
                       
                </div>
            </div>
            
            <div class="form-paragraph">Зарегистрироваться с помощью социальных сетей</div>
            <div class="social-icons"><a href="#" class="btn-social w-inline-block"><img src="~/images/google-icon.svg" width="16" alt="icon-google"></a><a href="#" class="btn-social w-inline-block"><img src="~/images/twitter-icon.svg" width="16" alt="icon-google"></a><a href="#" class="btn-social w-inline-block"><img src="~/images/facebook-icon.svg" alt="icon-google" height="16"></a><a href="#" class="btn-social w-inline-block"><img src="~/images/instagram-icon.svg" alt="icon-google" height="16"></a><a href="#" class="btn-social w-inline-block"><img src="~/images/linkedin.svg" alt="icon-google" height="16"></a></div>
            <div class="form-footer-text">
                <div class="form-paragraph">Уже зарегистрирован?</div>
                <a href="login" class="link-block w-inline-block">
                    <div class="btn-linkback">Войти</div>
                </a>
            </div>
        </div>
        <a href="home.html" class="btn-linkback w-inline-block">
            <div class="btn-goback">Вернуться на главную</div>
        </a>
    </div>
    <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.4.1.min.220afd743d.js?site=5ec2d8ba5aab1b0a2602941c" type="text/javascript" integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo=" crossorigin="anonymous"></script>
    <script src="~/js/digitaleyekz.js" type="text/javascript"></script>
    <!-- [if lte IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif] -->
</body>
</html>
